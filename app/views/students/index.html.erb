<%= form_tag students_path, method: :get do  %>
  <%= label_tag :name %> 
  <br>
  <%= text_field_tag :name, params[:name] %>
  <br>
  <%= submit_tag "Search", name: nil %>
<% end %>

<% @students.each do |s| %>
 <% if s.name.blank? %>
 
  <%= render partial: 'student', locals: { s: student} %>
  <% else %>
  <%= s.name %>
  <% end %>
<% end %>